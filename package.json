{
  "name": "@lukarmar/ddd-pratic",
  "version": "1.0.0",
  "description": "",
  "main": "src/application/server.ts",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore node_modules .",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "dev:create:migrations": "docker-compose exec app-delivery /bin/sh -c \"npm run typeorm -- migration:create ./src/infra/config/database/migrations/$npm_config_name\"",
    "dev:generate:migrations": "docker-compose exec app-delivery /bin/sh -c \"npm run typeorm -- -d ./src/infra/config/database migration:generate ./src/infra/config/database/migrations\"",
    "dev:run:migrations": "docker-compose exec app-delivery /bin/sh -c \"npm run typeorm migration:run -- -d ./src/infra/config/database\"",
    "dev:revert:migrations": "docker-compose exec app-delivery /bin/sh -c \"npm run typeorm migration:revert -- -d ./src/infra/config/database\"",
    "test": "npx jest --runInBand",
    "test:watch": "npx jest --watchAll --runInBand",
    "test:cov": "npx jest --no-cache --runInBand --coverage",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.1.0",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest-junit": "^16.0.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@types/express": "^4.17.17",
    "body-parser": "^1.20.2",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "luxon": "^1.28.1",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.15",
    "xml2js": "^0.6.0"
  }
}
